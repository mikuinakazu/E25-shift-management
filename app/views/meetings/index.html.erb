<h1>New Meeting</h1>

<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= meeting.user.name %>
    </div>
  <% end %>

<% meetings.each do |meeting| %>
    <div>
      <%= meeting.shift.start_end_time %>
    </div>
  <% end %>

<% end %>


<%= render 'form' %>

<%= link_to 'Back', meetings_path %>
