<h1>New Meeting</h1>

<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= meeting.user.name %>
    </div>
  <% end %>

<% meetings.each do |meeting| %>
    <div>
      <%= meeting.shift.start_end_time %>
    </div>
  <% end %>

<% end %>


<%= link_to 'ログアウト', destroy_user_session_path, method: :delete %>
