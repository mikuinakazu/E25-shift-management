<h1>New shift</h1>

<%= month_calendar events: @shifts do |date, shifts| %>
  <%= date %>

  <% shifts.each do |shift| %>
    <div>
      <%= shift.user.name %>
    </div>
  <% end %>

<% shifts.each do |shift| %>
    <div>
      <%= link_to "#{shift.shifttime.start_end_time}", controller: "shifttimes", action: "show", id: "#{shift.shift_id}", date: "#{shift.start_time}" %>
    </div>
  <% end %>

<% end %>


<%= link_to 'ログアウト', destroy_user_session_path, method: :delete %>
